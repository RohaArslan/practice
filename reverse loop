#include <stdio.h>
#include <stdlib.h>

int main()
{

    int num,rem,reverse_num=0;
    printf("enter number to reverse: ");
    scanf("%d",&num);
    while(num>=1)
    {
        rem=num%10;
        reverse_num=reverse_num*10+rem;
        num=num/10;

    }
 printf("reverse number is: %d\n",reverse_num);
 return 0;
}
